{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "semi": [2, "never"], // [设定] 句尾后面不要有分号
    "jsx-a11y/anchor-is-valid": 0,
    "import/prefer-default-export": 1
  }
}